{
	"expo": {
		"name": "uk-pool-tracker",
		"slug": "uk-pool-tracker-roikic",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/natively-dark.png",
		"userInterfaceStyle": "dark",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/images/natively-dark.png",
			"resizeMode": "contain",
			"backgroundColor": "#000000"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.frithfudge.pooltrackeruk",
			"infoPlist": {
				"ITSAppUsesNonExemptEncryption": false
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/natively-dark.png",
				"backgroundColor": "#000000"
			},
			"edgeToEdgeEnabled": true,
			"package": "com.anonymous.Natively"
		},
		"web": {
			"output": "single",
			"favicon": "./assets/images/final_quest_240x240.png",
			"bundler": "metro",
			"build": {
        "babel": true}
		},
		"plugins": [
			"expo-font",
			"expo-router"
		],
		"scheme": "natively",
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {}
		}
	}
}
